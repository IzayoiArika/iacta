		},
		"bgs": {
			"size": [1920, 1440]
		},
		"hitsounds": {
			"sampling_rate": 44100
		},

		"songlist": {
			"masking": {
				"artist": "SZANluvSWK",
				"bpm": "wys0318",
				"song_title": "{event_name} Chart {live_id}"
			}
		}
	},

	"livestream": {
		"sessions": 2
	},
	"guessletter": {
		"max_per_dict": 10
